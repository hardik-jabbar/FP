{
  "version": 2,
  "builds": [
    {
      "src": "FARMPOWER/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "FARMPOWER/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "FARMPOWER/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/FARMPOWER/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/FARMPOWER/server.js"
    }
  ]
}
