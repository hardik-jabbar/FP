{
  "version": 2,
  "builds": [
    {
      "src": "FARMPOWER/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "FARMPOWER"
      }
    },
    {
      "src": "FARMPOWER/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "FARMPOWER/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/FARMPOWER/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/FARMPOWER/server.js"
    }
  ]
}
